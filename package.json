{
  "name": "@project/qwik",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=20.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.server.development": "vite build -c adapters/express/vite.config.ts --mode development",
    "build.server.production": "vite build -c adapters/express/vite.config.ts --mode production",
    "build.server.staging": "vite build -c adapters/express/vite.config.ts --mode staging",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr --host",
    "dev.debug": "node --inspect-brk ../../node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en,de --assetsPath=i18n --autoKeys=true",
    "qwik-speak-deepl": "qwik-speak-deepl -a a00b4148-0259-a33a-0193-703e8f77a5a4:fx",
    "lint": "eslint --fix \"src/**/*.ts*\" \"i18n/**/*.json\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node --env-file=.env server/entry.express",
    "start": "vite --open --mode ssr",
    "svg.svgo": "node scripts/generate-icon-components.js && eslint --ext .tsx --fix ./src/components/images/icons/",
    "svg.createcomponents": "yarn run svg.svgo >/dev/null 2>&1 && node scripts/generate-icon-components.js && eslint --ext .tsx --fix ./src/components/images/icons/",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@asbjorn/eslint-plugin-groq": "^2.0.0",
    "@tailwindcss/typography": "^0.5.10",
    "@tegonal/qwik-speak-deepl": "^1.0.4",
    "@types/compression": "^1.7.5",
    "@types/cookie": "^0.6.0",
    "@types/express": "4.17.21",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.30",
    "@types/showdown": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "autoprefixer": "^10.4.19",
    "cssnano": "^6.1.1",
    "cssnano-preset-advanced": "^6.1.1",
    "csso": "^5.0.5",
    "eslint": "^8.57.0",
    "eslint-plugin-jsonc": "^2.14.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qwik": "1.5.1",
    "image-size": "^1.1.1",
    "postcss": "8.4.38",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.0",
    "postcss-preset-env": "^9.5.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "rimraf": "^5.0.5",
    "svgo": "^3.2.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "5.4.3",
    "vite": "5.2.3",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-webfont-dl": "^3.9.1",
    "vite-tsconfig-paths": "4.3.2"
  },
  "dependencies": {
    "@builder.io/qwik": "1.5.1",
    "@builder.io/qwik-city": "1.5.1",
    "@medusajs/medusa-js": "^6.1.8",
    "@medusajs/types": "^1.11.14",
    "@modular-forms/qwik": "^0.23.1",
    "@resvg/resvg-js": "^2.6.0",
    "@sanity/client": "^6.15.7",
    "@stripe/stripe-js": "^3.0.10",
    "@tegonal/portabletext-qwik": "^1.1.1",
    "@unpic/core": "^0.0.47",
    "@unpic/qwik": "^0.0.44",
    "compression": "^1.7.4",
    "cookie": "^0.6.0",
    "daisyui": "^4.7.3",
    "date-fns": "^3.6.0",
    "express": "4.19.1",
    "groqd": "^0.15.10",
    "imask": "^7.5.0",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "meilisearch": "^0.38.0",
    "node-cache": "^5.1.2",
    "qwik-image": "^0.0.8",
    "qwik-speak": "^0.22.0",
    "satori": "^0.10.13",
    "satori-html": "^0.3.2",
    "schema-dts": "^1.1.2",
    "showdown": "^2.1.0",
    "shrink-string": "^3.1.9",
    "tslog": "^4.9.2",
    "undici": "6.10.1",
    "valibot": "^0.30.0"
  },
  "packageManager": "yarn@4.1.1"
}
